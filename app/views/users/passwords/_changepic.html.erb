<%= render 'home/flash' %> 
<%= render 'home/headerup_in' %>
<%= form_for(resource, as: resource_name, url: edit_user_registration_path(resource_name), html: { method: :put }) do |f| %>
  <%= devise_error_messages! %>
  <div class="field form-group">
    <%= f.label :profile_pic, 'Choose Image', class: 'col-sm-3 control-label' %>
    <div class="col-sm-4">
      <%= f.file_field :profile_pic %>
    </div>
  </div>
  <div class="actions form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <%= f.submit "save", class: 'btn log sub btn-default' %>
    </div>
  </div>
<% end %>